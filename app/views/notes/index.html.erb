<% unless @notes.blank? %>
    <button>
      <%= link_to "New Note", new_note_path %>
    </button>
    <button>
      <%= link_to "New File", new_upload_path %>
    </button>
    <% @notes.each do |note| %>
        <a href="<%= (url_for [note]) %>">
            <p>
              <%= note.title %>
            </p>
            <p>
              <%= time_ago_in_words(note.created_at) %>
            </p>
        </a>
    <% end %>
    <%= render 'uploads/index' %>
<% else %>
    <h2>Add a Note</h2>
    <p>It appears you haven't created any notes yet... Lets fix that!</p>
    <button>
      <%= link_to "New Note", new_note_path %>
    </button>
    <button>
    <%= link_to "New File", new_upload_path %>
    </button>
<% end %>
